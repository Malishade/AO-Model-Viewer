#list-entry {
    align-items: flex-start;
    justify-content: center;
    padding-left: 10px;
    border-color: rgb(25, 25, 25);
    border-width: 1px;
    border-radius: 0;
    width: 400px;
    height: 41px;
}

#DisplayName {
    -unity-font-style: bold;
    font-size: 12px;
    color: rgb(255, 241, 171);
}

#list-entry:hover {
    background-color: rgb(67, 92, 120);
}