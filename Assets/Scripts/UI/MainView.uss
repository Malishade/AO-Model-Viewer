    border-bottom-color: var(--gray33);
    border-left-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
}

#ResourceTypeContainer {
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-top-color: var(--gray33);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
}

.unity-base-dropdown__container-inner {
    background-color: var(--gray66);
}

#unity-slider
{
    margin-bottom: 14px;
    margin-top: 14px;
    width: 13px;
    min-width: 14px;
}

#unity-high-button
{
    background-color: var(--gray188);
