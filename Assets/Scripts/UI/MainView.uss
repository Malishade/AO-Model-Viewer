:root {  
  --White: rgb(255, 255, 255);
  --Transparent: rgba(255, 255, 255,0);

  --gray225: rgb(225, 225, 225);
  --gray188: rgb(188, 188, 188);

  --gray125: rgba(125, 125, 125, 0.5);

  --gray76: rgb(76, 76, 76);
  --gray66: rgb(66, 66, 66);

  --gray56: rgb(56, 56, 56);
  --gray46: rgb(46, 46, 46);
  --gray42: rgb(42, 42, 42);
  --gray33: rgb(33, 33, 33);
  --gray25: rgb(25, 25, 25);
  --gray13: rgb(13, 13, 13);
  --lime: rgb(94, 137, 35);
  --light-lime: rgb(214, 247, 168);
}

.unity-list-view {
    background-color: var(--gray42);
    border-top-color: var(--gray33);
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
    border-radius: 0px;
    border-width: 1px;
    color: var(--gray188);
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    padding-top: 0px;
    padding-left: 0px;
    padding-right: 0px;
    padding-bottom: 0px;
}

.unity-list-view .unity-scroll-view {
    width: 100%;
}

.unity-collection-view:focus .unity-collection-view__item--selected{
    background-color: var(--lime);
    color: var(--light-lime);
}

.unity-collection-view__item--alternative-background
{
    background-color: var(--gray46);
}

.unity-base-text-field__input:focus {
    border-color: var(--lime);
}

.unity-scroller--vertical .unity-base-slider__dragger {
    background-color: var(--gray188);
    border-radius: 1px;
    left: 0px;
    min-height: 12px;
    height: 11px;
    width: 12px;
    margin-left: 1px;
}


.unity-base-slider__drag-container:active:hover > .unity-base-slider__dragger{
    border-color: var(--gray33);
    border-width: 1px;
    background-color: var(--lime);
}

.unity-list-view:focus > .unity-base-slider__dragger:{
    border-color: var(--gray33);
    border-width: 1px;
    background-color: var(--lime);

}

.unity-scroller__slider:focus > .unity-base-slider__input > .unity-base-slider__drag-container > .unity-base-slider__dragger:{
    border-color: var(--gray33);
    border-width: 1px;
    background-color: var(--gray188);
}

.unity-scroller__slider:focus > .unity-base-slider__input > .unity-base-slider__drag-container:active:hover > .unity-base-slider__dragger:{
    border-color: var(--gray33);
    border-width: 1px;
    background-color: var(--lime);
}


.unity-base-slider__drag-container:hover > .unity-base-slider__dragger{
    border-color: var(--gray33);
    border-width: 1px;
    background-color: var(--gray225);
}


.unity-scroller--vertical .unity-base-slider__tracker {
    background-color: var(--gray42);
    border-color: var(--gray33);
}

.unity-scroller--vertical .unity-base-slider__tracker:hover {
    background-color: var(--gray125);
}

.unity-scroller--vertical .unity-scroller__high-button {
    height: 14px;
    width: 14px;
    background-image: url('project://database/Assets/Sprites/Slider-HighButton.png');
    -unity-background-image-tint-color: var(--White);
}

.unity-scroller--vertical .unity-scroller__low-button {
    background-color: var(--gray188);
    border-color: var(--gray33);
    height: 14px;
    width: 14px;
    background-image: url('project://database/Assets/Sprites/Slider-LowButton.png');
    -unity-background-image-tint-color: var(--White);
}

.unity-label {
    color: var(--gray188);
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    padding-top: 0px;
    padding-left: 5px;
    padding-right: 5px;
    padding-bottom: 0px;
    -unity-font-definition: url('project://database/Assets/Fonts/PixelFont.TTF');
    font-size: 8px;
    text-shadow: -0.5px 0.5px 0px black;
}

.unity-base-text-field__input {
    background-color: var(--gray76);
    color: var(--gray188);
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    padding-top: 0px;
    padding-left: 5px;
    padding-right: 5px;
    padding-bottom: 0px;
    border-top-color: var(--gray13);
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
    border-radius: 3px;
}

.unity-base-popup-field__input {
    background-color: var(--gray76);
    color: var(--gray188);
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    padding-top: 0px;
    padding-left: 5px;
    padding-right: 5px;
    padding-bottom: 0px;
    border-top-color: var(--gray13);
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
    border-radius: 3px;
}

.unity-button {
    color: var(--gray188);
    background-color: var(--gray76);
    border-top-color: var(--gray33);
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
    border-radius: 0px;
    -unity-font-definition: url('project://database/Assets/Fonts/PixelFont.TTF');
    font-size: 8px;
    text-shadow: -0.5px 0.5px 0px black;
}

.unity-button:hover {
    color: var(--gray225);
    background-color: var(--gray125);
    border-color: var(--gray33);
}

.unity-button:focus {
    color: var(--light-lime);
    background-color: var(--lime);
    border-color: var(--gray33);
}

.unity-list-view__item:{
    font-size: 12px;
    padding-top: 4px;
    border-width: 1px;
    font-size: 8px;
    -unity-font-definition: url('project://database/Assets/Fonts/PixelFont.TTF');
}

.unity-collection-view__item:hover:enabled {
    background-color: var(--gray125);
    color: var(--gray225);
}

.unity-list-view__item:hover:checked {
    color: var(--light-lime);
    background-color: var(--lime);
    border-color: var(--gray33);
}

.unity-list-view__item:checked {
    color: var(--light-lime);
    background-color: var(--lime);
    border-color: var(--gray33);
}

.unity-popup-field {
}

.unity-base-field--no-label {
}

.unity-base-popup-field__arrow {
    display: none;
}

#MainContainer {
    background-color: var(--gray56);
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    padding-top: 5px;
    padding-left: 5px;
    padding-right: 5px;
    padding-bottom: 5px;
}

