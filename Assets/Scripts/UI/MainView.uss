}

.unity-radio-button__checkmark {
    background-image: url('project://database/Assets/Sprites/Checkbox-Circle-Active.png');
        height: 11px;
        width: 11px;
        border-width: 0px;
        border-radius: 0px;
        background-color: var(--Transparent);
        position: absolute;
}

