.unity-scroll-view__vertical-scroller {
    width: 13px;
    padding-right: 11px;
}

.unity-base-dropdown__label {
    height: 23px;
}

.unity-base-popup-field__text {
    -unity-font-definition: url('project://database/Assets/Fonts/PixelFont.TTF');
    font-size: 8px;
    text-shadow: -0.5px 0.5px 0px black;
    padding-top: 5px;
    padding-bottom: 3px;
    padding-left: 6px;
}

.unity-base-popup-field:hover > .unity-base-popup-field__input
{    
    background-color: var(--gray125);
    color: var(--gray225);
    border-top-color: var(--gray13);    
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
}

.unity-base-popup-field:active > .unity-base-popup-field__input
{    
    color: var(--light-lime);
    background-color: var(--lime);
    border-top-color: var(--gray13);    
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
}

.unity-base-field:focus > .unity-base-field__label {
    color: var(--gray188);
}

.unity-base-dropdown__item:hover {
    background-color: var(--gray125);
    color: var(--gray225);
}

.unity-base-dropdown__container-outer {
    background-color: var(--gray125);
    color: var(--gray225);
    border-top-width: 0px;
    border-bottom-width: 1px;
    border-right-width: 1px;   
    border-left-width: 1px;
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
}

#unity-text-input {
    font-size: 12px;
    padding-bottom: 3px;
    padding-top: 5px;
    border-width: 1px;
    font-size: 8px;
    -unity-font-definition: url('project://database/Assets/Fonts/PixelFont.TTF');
    text-shadow: -0.5px 0.5px 0px black;
}
#unity-text-input:hover {
    border-top-color: var(--gray13);    
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
    background-color: var(--gray125);
}

#unity-text-input:disabled {
    border-top-color: var(--gray13);    
    border-left-color: var(--gray13);
    border-right-color: var(--gray13);
    border-bottom-color: var(--gray13);
    background-color: var(--gray25);
}

#unity-text-input:hover:focus {
    background-color: var(--gray56);
}

#unity-text-input:focus {
    background-color: var(--gray56);
}

.unity-base-text-field {
    --unity-selection-color: var(--lime);
}

.unity-base-text-field:focus > .unity-base-text-field__input {
    border-top-color: var(--gray13);    
    border-left-color: var(--gray33);
    border-right-color: var(--gray33);
    border-bottom-color: var(--gray33);
}

.unity-base-dropdown__checkmark {

    background-image: url('project://database/Assets/Sprites/Checkbox-Circle-Active.png');
    -unity-background-image-tint-color: var(--White);
    height: 11px;
    width: 11px;
    margin-top: -2px;
    margin-left: 6px;
    margin-right: 11px;

}

.unity-radio-button__checkmark-background {
    background-image: url('project://database/Assets/Sprites/Checkbox-Circle-Inactive.png');
        height: 11px;
        width: 11px;
        border-width: 0px;
        border-radius: 0px;
        background-color: var(--Transparent);
        padding-top: 0px;
        -unity-background-image-tint-color: var(--gray188);
}

.unity-radio-button:hover > .unity-radio-button__input > .unity-radio-button__checkmark-background {
        -unity-background-image-tint-color: var(--White);

}

.unity-radio-button-group {
padding-top: 7px;
}
.unity-radio-button__text {
    padding-top: 1px;
}

.unity-radio-button:checked > .unity-radio-button__input > .unity-radio-button__text{
        color: var(--light-lime);
}

.unity-radio-button:hover > .unity-radio-button__input > .unity-radio-button__text{
        color: var(--gray225);
}

.unity-radio-button:hover:focus > .unity-radio-button__input > .unity-radio-button__text{
        color: var(--light-lime);
}

.unity-radio-button:hover:checked > .unity-radio-button__input > .unity-radio-button__text{
